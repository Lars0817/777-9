<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <checklist>
        <title>Before Starting Engine</title>
        <item>
            <name>Parking Brake</name>
            <value>Test and Set</value>
            <condition>
                <equals>
                    <property>controls/gear/brake-parking</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Throttle</name>
            <value>Idle</value>
            <condition>
                <and>
                    <equals>
                        <property>controls/engines/engine[0]/throttle-lever</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>controls/engines/engine[1]/throttle-lever</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
        </item>
        <item>
            <name>Fuel flow</name>
            <value>Cut Off</value>
            <condition>
                <and>
                    <equals>
                        <property>controls/engines/engine[0]/cutoff</property>
                        <value>true</value>
                    </equals>
                    <equals>
                        <property>controls/engines/engine[1]/cutoff</property>
                        <value>true</value>
                    </equals>
                </and>
            </condition>
        </item>
        <item>
            <name>BATT Master Switch</name>
            <value>On</value>
            <condition>
                <equals>
                    <property>controls/electric/battery-switch</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Start APU</name>
            <value>On</value>
            <condition>
                <property>controls/APU/run</property>
            </condition>
        </item>
        <item>
            <name>left/right power-bus cross-feeds</name>
            <value>On</value>
            <condition>
                <and>
                    <property>controls/electric/engine[0]/bus-tie</property>
                    <property>controls/electric/engine[1]/bus-tie</property>
                </and>
            </condition>
        </item>
        <item>
            <name>Landing Gear Lever</name>
            <value>Check Down</value>
            <condition>
               <property>controls/gear/gear-down</property>
            </condition>
        </item>
        <item>
            <name>Flaps</name>
            <value>Up</value>
            <condition>
                <equals>
                    <property>controls/flight/flaps</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Down</value>
            <condition>
                <equals>
                    <property>controls/flight/speedbrake</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Fuel Quantity</name>
            <value>Check</value>
        </item>
        <item>
            <name>Engine Instruments</name>
            <value>Check</value>
        </item>
        <item>
            <name>Flight Controls</name>
            <value>Free and Correct</value>
        </item>
        <item>
            <name>Avionics</name>
            <value>On</value>
        </item>
        <item>
          <name>ADIRS</name>
          <value>On</value>
          <condition>
           <property>controls/flight/adiru</property>
          </condition>
        </item>
        <item>
            <name>Exterior Lights</name>
            <value>Off</value>
            <condition>
                <and>
                    <not><property>controls/lighting/taxi-lights</property></not>
                    <not><property>controls/lighting/landing-lights</property></not>
                    <not><property>controls/lighting/strobe</property></not>
                    <not><property>controls/lighting/nav-lights</property></not>
                    <not><property>controls/lighting/beacon</property></not>
                </and>
            </condition>
        </item>
        <item>
            <name>Fasten Seat Belts</name>
            <value>On</value>
            <condition>
                <equals>
                    <property>controls/cabin/SeatBelt-knob</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>No Smoking</name>
            <value>On</value>
            <condition>
                <equals>
                    <property>controls/cabin/NoSmoking-knob</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Check Weather</name>
            <value>Atis Flight Service</value>
        </item>
        <item>
            <name>Auto-pilot</name>
            <value>Preset</value>
        </item>
        <item>
            <name>Altimeter</name>
            <value>Check and set</value>
            <condition>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>-3</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>Transition Altitude</name>
            <value>Check and Set (Hit F11)</value>
        </item>
        <item>
            <name>Trims</name>
            <value>Set</value>
        </item>
        <item>
            <name>Autobrakes</name>
            <value>Off</value>
            <condition>
                <equals>
                <property>autopilot/autobrake/step</property>
                <value>-1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Radio</name>
            <value>Set</value>
        </item>
        <item>
            <name>Takeoff briefing</name>
            <value>Completed</value>
        </item>
        <item>
            <name>Departure procedure</name>
            <value>Review</value>
        </item>
        <item>
            <name>GPWS self-test (press and hold for 20 seconds)</name>
            <value>Done</value>
            <condition>
             <property>instrumentation/mk-viii/inputs/discretes/self-tested</property>
            </condition>
        </item>
        <item>
            <name>Anti-ice</name>
            <value>Test/check</value>
        </item>
        <item>
            <name>Request Clearance</name>
            <value>Done</value>
        </item>
        <item>
            <name>Transponder</name>
            <value>Standby</value>
            <condition>
                <equals>
                    <property>instrumentation/transponder/mode-switch</property>
                    <value>0</value>
                </equals>
            </condition>
         </item>
    </checklist>
    <checklist>
        <title>Startup</title>
        <item>
            <name>Beacon Light</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/beacon</property>
            </condition>
        </item>
        <item>
            <name>Contact Ground</name>
            <value>Done</value>
        </item>
        <item>
            <name>Strobe Light</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/strobe</property>
            </condition>
        </item>
        <item>
            <name>Left/right/(center) fuel pumps ON</name>
            <value>On</value>
            <condition>
                <and>
                 <equals>
                  <property>controls/fuel/tank/boost-pump</property>
                  <value>1</value>
                 </equals>
                 <equals>
                  <property>controls/fuel/tank/boost-pump[1]</property>
                  <value>1</value>
                 </equals>
                 <equals>
                  <property>controls/fuel/tank[2]/boost-pump</property>
                  <value>1</value>
                 </equals>
                 <equals>
                  <property>controls/fuel/tank[2]/boost-pump[1]</property>
                  <value>1</value>
                 </equals>
                 <not>
                 <equals>
                  <property>controls/fuel/tank[1]/boost-pump-switch</property>
                  <property>consumables/fuel/tank[1]/empty</property>
                 </equals>
                 </not>
                 <not>
                 <equals>
                  <property>controls/fuel/tank[1]/boost-pump-switch[1]</property>
                  <property>consumables/fuel/tank[1]/empty</property>
                 </equals>
                 </not>
                </and>
            </condition>
        </item>
        <item>
            <name>Set #1(left)Engine start knob to</name>
            <value>Start</value>
            <condition>
             <or>
                <equals>
                    <property>controls/engines/StartIgnition-knob[0]</property>
                    <value>-1</value>
                </equals>
                <equals>
                    <property>engines/engine/run</property>
                    <value>1</value>
                </equals>
             </or>
            </condition>
        </item>
        <item>
            <name>At N2 &gt; 20 % fuel flow engine</name>
            <value>Check</value>
            <condition>
                <equals>
                    <property>controls/engines/engine[0]/cutoff</property>
                    <value>false</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>N1 is increasing as N2 incr.</name>
            <value>Check</value>
        </item>
        <item>
            <name>Set #2(Right)Engine start knob to</name>
            <value>Start</value>
            <condition>
             <or>
                <equals>
                    <property>controls/engines/StartIgnition-knob[1]</property>
                    <value>-1</value>
                </equals>
                <equals>
                    <property>engines/engine[1]/run</property>
                    <value>1</value>
                </equals>
             </or>
            </condition>
        </item>
        <item>
            <name>At N2 &gt; 20 % fuel flow engine</name>
            <value>Check</value>
            <condition>
                <equals>
                    <property>controls/engines/engine[1]/cutoff</property>
                    <value>false</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>N1 is increasing as N2 incr.</name>
            <value>Check</value>
        </item>
        <item>
            <name>Switch Off APU</name>
            <value>Check</value>
            <condition>
                <equals>
                    <property>controls/APU/run</property>
                    <value>false</value>
                </equals>
            </condition>
        </item>
        <!--item>
            <name>BATT Master Switch</name>
            <value>Off</value>
            <condition>
                <equals>
                    <property>controls/electric/battery-switch</property>
                    <value>0</value>
                </equals>
            </condition>
        </item-->
    </checklist>
    <checklist>
        <title>After Startup</title>
        <item>
            <name>Time</name>
            <value>Noted</value>
        </item>
        <item>
            <name>Start switches</name>
            <value>Norm</value>
            <condition>
                <and>
                <equals>
                    <property>controls/engines/StartIgnition-knob[0]</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>controls/engines/StartIgnition-knob[1]</property>
                    <value>0</value>
                </equals>
                </and>
            </condition>
        </item>
        <item>
            <name>Engine parameters</name>
            <value>Checked</value>
        </item>
        <item>
          <name>Hydraulic Pumps</name>
          <value>On</value>
          <condition>
             <and>
              <greater-than>
               <property>controls/hydraulics/system/LACMP-switch</property>
               <value type="int">0</value>
              </greater-than>
              <equals>
               <property>controls/hydraulics/system/LENG_switch</property>
               <value type="bool">true</value>
              </equals>
              <greater-than>
               <property>controls/hydraulics/system[1]/C1ADP-switch</property>
               <value type="int">0</value>
              </greater-than>
              <equals>
               <property>controls/hydraulics/system[1]/C1ELEC-switch</property>
               <value type="bool">true</value>
              </equals>
              <greater-than>
               <property>controls/hydraulics/system[1]/C2ADP-switch</property>
               <value type="int">0</value>
              </greater-than>
              <equals>
               <property>controls/hydraulics/system[1]/C2ELEC-switch</property>
               <value type="bool">true</value>
              </equals>
              <greater-than>
               <property>controls/hydraulics/system[2]/RACMP-switch</property>
               <value type="int">0</value>
              </greater-than>
              <equals>
               <property>controls/hydraulics/system[2]/RENG_switch</property>
               <value type="bool">true</value>
              </equals>
             </and>
          </condition>
        </item>
        <item>
          <name>Window Heat</name>
          <value>On</value>
          <condition>
           <and>
            <property>controls/anti-ice/window-heat-lf</property>
            <property>controls/anti-ice/window-heat-ls</property>
            <property>controls/anti-ice/window-heat-rf</property>
            <property>controls/anti-ice/window-heat-rs</property>
           </and>
          </condition>
        </item>
        <item>
            <name>Flaps</name>
            <value>Set for takeoff</value>
            <condition>
                <greater-than>
                    <property>controls/flight/flaps</property>
                    <value>0</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>Doors</name>
            <value>Closed</value>
        </item>
        <item>
            <name>Outside Equipment/Personnel</name>
            <value>Clear</value>
        </item>
        <item>
            <name>Flight controls</name>
            <value>Free and Checked</value>
        </item>
        <item>
            <name>Lights</name>
            <value>As requested</value>
        </item>
    </checklist>

    <checklist>
        <title>Pushback</title>
        <item>
            <name>Doors</name>
            <value>Closed</value>
        </item>
        <item>
            <name>Outside Equipment/Personnel</name>
            <value>Clear</value>
        </item>
        <item>
            <name>Cabin Preparation</name>
            <value>Verify Cabin Ready</value>
        </item>
        <item>
            <name>Beacon Light</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/beacon</property>
            </condition>
        </item>
        <item>
            <name>Strobe lights</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/strobe</property></not>
            </condition>
        </item>        
        <item>
            <name>Pushback Clearance</name>
            <value>Obtained</value>
        </item>
    </checklist>
    
    <checklist>
        <title>Taxi Out</title>
        <item>
            <name>Transponder</name>
            <value>ALT RPTG OFF</value>
            <condition>
                <equals>
                    <property>instrumentation/transponder/mode-switch</property>
                    <value>1</value>
                </equals>
            </condition>
         </item>
        <item>
            <name>Taxi Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/taxi-lights</property>
            </condition>
        </item>
        <item>
            <name>Nav Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/nav-lights</property>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Armed</value>
            <condition>
               <property>controls/flight/speedbrake-lever</property>
            </condition>
        </item>
        <item>
            <name>Brakes</name>
            <value>Tested</value>
        </item>
        <item>
            <name>Instrument</name>
            <value>Checked</value>
        </item>
    </checklist>
    <checklist>
        <title>Before Take-off</title>
        <item>
            <name>Contact Tower</name>
            <value>Done</value>
        </item>
        <item>
        <name>Take-off/Line-up-clearance</name>
            <value>Obtained</value>
        </item>
        <item>
            <name>Transponder</name>
            <value>TA/RA</value>
            <condition>
                <equals>
                    <property>instrumentation/transponder/mode-switch</property>
                    <value>4</value>
                </equals>
            </condition>
         </item>
        <item>
            <name>Cabin Lights</name>
            <value>Off</value>
            <condition>
               <equals>
               <property>systems/electrical/outputs/cabin-lights</property>
               <value>0</value>
               </equals>
            </condition>        
        </item>
        <item>
            <name>Strobe Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/strobe</property>
            </condition>
        </item>
        <item>
            <name>Landing Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/landing-lights</property>
            </condition>
        </item>
        <item>
            <name>Taxi Lights</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/taxi-lights</property></not>
            </condition>
        </item>        
        <item>
            <name>Anti-Ice</name>
            <value>On</value>
            <condition>
             <and>
              <greater-than>
               <property>controls/anti-ice/engin/antiice-knob</property>
               <value>0</value>
              </greater-than>
              <greater-than>
               <property>controls/anti-ice/engin[1]/antiice-knob</property>
               <value>0</value>
              </greater-than>
              <greater-than>
               <property>controls/anti-ice/wing-antiice-knob</property>
               <value>0</value>
              </greater-than>
             </and>
            </condition>
        </item>
        <item>
            <name>Autobrakes</name>
            <value>RTO</value>
            <condition>
                <equals>
                <property>autopilot/autobrake/step</property>
                <value>-2</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Time</name>
            <value>Noted</value>
        </item>
        <item>
            <name>Auto-pilot</name>
            <value>Off</value>
            <condition>
            <not>
               <property>instrumentation/afds/inputs/AP</property>
            </not>
            </condition>
        </item>
        <item>
            <name>Auto-throttles</name>
            <value>Armed</value>
            <condition>
               <and>
               <property>instrumentation/afds/inputs/at-armed[0]</property>
               <property>instrumentation/afds/inputs/at-armed[1]</property>
               </and>
            </condition>
        </item>
    </checklist>
    <checklist>
        <title>After Take-off</title>
        <item>
            <name>Positive Climb</name>
            <value>Check</value>
            <condition>
              <greater-than>
               <property>instrumentation/inst-vertical-speed-indicator/indicated-speed-fpm</property>
               <value>900</value>
              </greater-than>
            </condition>
        </item>
        <item>
            <name>Landing gear</name>
            <value>Up</value>
            <condition>
               <not><property>controls/gear/gear-down</property></not>
            </condition>
        </item>
        <item>
            <name>Flaps</name>
            <value>Up</value>
            <condition>
                <equals>
                    <property>controls/flight/flaps</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Retracted</value>
            <condition>
                <equals>
                    <property>controls/flight/speedbrake</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Engine Parameters</name>
            <value>Checked</value>
        </item>
        <item>
            <name>Auto-pilot</name>
            <value>Engage and set for climb</value>
            <condition>
               <property>instrumentation/afds/inputs/AP</property>
            </condition>
        </item>
        <item>
            <name>Power Speed</name>
            <value>Set for climb</value>
        </item>
        <item>
            <name>Lights</name>
            <value>As requested</value>
        </item>
        <item>
            <name>Contact Departure</name>
            <value>Done</value>
        </item>
    </checklist>
    <checklist>
        <title>10000 Climb</title>
        <item>
            <name>Contact Center</name>
            <value>Done</value>
        </item>
        <item>
            <name>Cabin Lights</name>
            <value>As required</value>
        </item>
        <item>
            <name>Cockpit</name>
            <value>Check</value>
        </item>
        <item>
            <name>Engine Parameters</name>
            <value>Checked</value>
        </item>
        <item>
            <name>Landing Light</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/landing-lights</property></not>
            </condition>
        </item>
        <item>
            <name>Power Speed</name>
            <value>Adjust for climb</value>
        </item>
        <item>
            <name>Fasten Seat Belts</name>
            <value>Auto</value>
            <condition>
                <equals>
                    <property>controls/cabin/SeatBelt-knob</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>No Smoking</name>
            <value>Auto</value>
            <condition>
                <equals>
                    <property>controls/cabin/NoSmoking-knob</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Barometer</name>
            <value>Standard</value>
        </item>
    </checklist>
    <checklist>
        <title>Cruise</title>
        <item>
            <name>Cockpit</name>
            <value>Check</value>
        </item>
        <item>
            <name>Power Speed</name>
            <value>Adjust for cruise</value>
        </item>
        <item>
            <name>De Ice</name>
            <value>As requested</value>
        </item>
        <item>
            <name>Fuel Quantity</name>
            <value>Checked</value>
        </item>
    </checklist>
    <checklist>
        <title>Descent</title>
        <item>
            <name>Contact Approach</name>
            <value>Done</value>
        </item>
        <item>
            <name>Approach Briefing</name>
            <value>Completed</value>
        </item>
        <item>
            <name>Weather</name>
            <value>Received</value>
        </item>
        <item>
            <name>Transition Level</name>
            <value>Check and Set</value>
        </item>
        <item>
            <name>Power Speed</name>
            <value>Set for descent</value>
        </item>
        <item>
            <name>De Ice</name>
            <value>As requested</value>
        </item>
        <item>
            <name>Active runway/ILS</name>
            <value>Set</value>
        </item>
        <item>
            <name>Minimums</name>
            <value>Set (radio Alt)</value>
        </item>
        <item>
            <name>Landing performance (weight)</name>
            <value>Check</value>
        </item>
    </checklist>
    <checklist>
        <title>In Range 10000 30 NM</title>
        <item>
            <name>Contact Approach</name>
            <value>Done</value>
        </item>
        <item>
            <name>Landing Light</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/landing-lights</property>
            </condition>
        </item>
        <item>
            <name>Air speed</name>
            <value>250 IAS Max</value>
            <condition>
                <less-than-equals>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <value>250</value>
                </less-than-equals>
            </condition>
        </item>
        <item>
            <name>Landing Briefing</name>
            <value>Completed</value>
        </item>
    </checklist>
    <checklist>
        <title>Before Landing</title>
        <item>
            <name>Contact Tower</name>
            <value>Done</value>
        </item>
        <item>
            <name>Cabin Lights</name>
            <value>Off</value>
            <condition>
               <equals>
               <property>systems/electrical/outputs/cabin-lights</property>
               <value>0</value>
               </equals>
            </condition>        
        </item>
        <item>
            <name>Landing Gear Lever</name>
            <value>Check Down</value>
            <condition>
               <property>controls/gear/gear-down</property>
            </condition>
        </item>
        <item>
            <name>Flaps</name>
            <value>Set for landing</value>
            <condition>
                <greater-than>
                    <property>controls/flight/flaps</property>
                    <value>0.666</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Armed</value>
            <condition>
               <property>controls/flight/speedbrake-lever</property>
            </condition>
        </item>
        <item>
            <name>Autobrakes</name>
            <value>Set</value>
            <condition>
                <greater-than>
                <property>autopilot/autobrake/step</property>
                <value>0</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>Taxi Lights</name>
            <value>On when clear to land</value>
            <condition>
               <property>controls/lighting/taxi-lights</property>
            </condition>
        </item>
        <item>
            <name>Minimums</name>
            <value>Check</value>
        </item>
        <item>
            <name>Radio Alt Alive (2500 RA)</name>
            <value>Check</value>
        </item>        
    </checklist>
    <checklist>
        <title>Final check</title>
        <item>
            <name>Flaps</name>
            <value>Down</value>
            <condition>
                <greater-than>
                    <property>controls/flight/flaps</property>
                    <value>0.666</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>Landing Gear Lever</name>
            <value>Check Down</value>
            <condition>
               <property>controls/gear/gear-down</property>
            </condition>
        </item>
        <item>
            <name>Minimums</name>
            <value>Check</value>
        </item>
        <item>
            <name>Winds</name>
            <value>Checked</value>
        </item>
    </checklist>
    <checklist>
        <title>Go Around/Missed Approach</title>
        <item>
            <name>Spoilers</name>
            <value>Retracted</value>
            <condition>
                <equals>
                    <property>controls/flight/speedbrake</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Flaps</name>
            <value>As Required</value>
            <condition>
                <less-than-equals>
                    <property>controls/flight/flaps</property>
                    <value>0.832</value>
                </less-than-equals>
            </condition>
        </item>
        <item>
            <name>Landing gear</name>
            <value>Up</value>
            <condition>
               <not><property>controls/gear/gear-down</property></not>
            </condition>
        </item>
        <item>
            <name>Engine Parameters</name>
            <value>Checked</value>
        </item>
        <item>
            <name>Auto-pilot</name>
            <value>Engage and set for climb</value>
            <condition>
               <property>instrumentation/afds/inputs/AP</property>
            </condition>
        </item>
        <item>
            <name>Power Speed</name>
            <value>Set for climb</value>
        </item>
        <item>
            <name>Lights</name>
            <value>As requested</value>
        </item>
        <item>
            <name>Contact Departure</name>
            <value>Done</value>
        </item>
    </checklist>
    <checklist>
        <title>Taxi In</title>
        <item>
            <name>Contact Ground</name>
            <value>Done</value>
        </item>
        <item>
            <name>Flaps</name>
            <value>Up</value>
            <condition>
               <equals>
               <property>controls/flight/flaps</property>
               <value>0</value>
               </equals>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Down</value>
            <condition>
                <equals>
                    <property>controls/flight/speedbrake</property>
                    <value>0</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Autobrakes</name>
            <value>Off</value>
            <condition>
                <less-than-equals>
                <property>autopilot/autobrake/step</property>
                <value>0</value>
                </less-than-equals>
            </condition>
        </item>
        <item>
            <name>Trims</name>
            <value>Neutral</value>
            <condition>
                <and>
                <less-than-equals>
                    <property>controls/flight/aileron-trim</property>
                    <value>0.02</value>
                </less-than-equals>
                <greater-than-equals>
                    <property>controls/flight/aileron-trim</property>
                    <value>-0.02</value>
                </greater-than-equals>
                <less-than-equals>
                    <property>controls/flight/elevator-trim</property>
                    <value>0.02</value>
                </less-than-equals>
                <greater-than-equals>
                    <property>controls/flight/elevator-trim</property>
                    <value>-0.02</value>
                </greater-than-equals>
                <less-than-equals>
                    <property>controls/flight/rudder-trim</property>
                    <value>0.02</value>
                </less-than-equals>
                <greater-than-equals>
                    <property>controls/flight/rudder-trim</property>
                    <value>-0.02</value>
                </greater-than-equals>
                </and>
            </condition>
        </item>
        <item>
            <name>Transponder</name>
            <value>ALT RPTG OFF</value>
            <condition>
                <equals>
                    <property>instrumentation/transponder/mode-switch</property>
                    <value>1</value>
                </equals>
            </condition>
         </item>
        <item>
            <name>Landing Lights</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/landing-lights</property></not>
            </condition>
        </item>
        <item>
            <name>Taxi Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/taxi-lights</property>
            </condition>
        </item>
        <item>
            <name>Nav Lights</name>
            <value>On</value>
            <condition>
               <property>controls/lighting/nav-lights</property>
            </condition>
        </item>
        <item>
            <name>Strobe Lights</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/strobe</property></not>
            </condition>
        </item>
        <item>
            <name>Spoilers</name>
            <value>Armed</value>
            <condition>
               <equals>
               <property>controls/flight/speedbrake-lever</property>
               <value>1</value>
               </equals>
            </condition>
        </item>
        <item>
            <name>Auto-pilot</name>
            <value>Off</value>
            <condition>
            <not>
               <property>instrumentation/afds/inputs/AP</property>
            </not>
            </condition>
        </item>
        <item>
            <name>A/T Arm Switches</name>
            <value>Off</value>
            <condition>
               <and>
               <not><property>instrumentation/afds/inputs/at-armed[0]</property></not>
               <not><property>instrumentation/afds/inputs/at-armed[1]</property></not>
               </and>
            </condition>
        </item>        
        <item>
            <name>F/D switches</name>
            <value>Off</value>
            <condition>
             <not><property>instrumentation/afds/inputs/FD</property></not>
            </condition>
        </item>
        <item>
            <name>APU</name>
            <value>Start (as necessary)</value>
            <condition>
                <equals>
                    <property>controls/APU/run</property>
                    <value>true</value>
                </equals>
            </condition>
        </item>        
        <item>
            <name>Cabin Lights</name>
            <value>On</value>
            <condition>
               <equals>
               <property>systems/electrical/outputs/cabin-lights</property>
               <value>28</value>
               </equals>
            </condition>        
        </item>
        <item>
            <name>Contact Ramp</name>
            <name>Done</name>
        </item>
    </checklist>
    <checklist>
        <title>Parking</title>
        <item>
            <name>Taxi Lights</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/taxi-lights</property></not>
            </condition>
        </item>        
        <item>
            <name>Parking Brake</name>
            <value>Set</value>
            <condition>
                <equals>
                    <property>controls/gear/brake-parking</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Request shutdown</name>
            <value>clearance obtained</value>
        </item>
        <item>
            <name>Fuel Control Switches</name>
            <value>CutOff</value>
        </item>        
        <item>
            <name>Fasten Seat Belts</name>
            <value>Off</value>
            <condition>
                <equals>
                    <property>controls/cabin/SeatBelt-knob</property>
                    <value>-1</value>
                </equals>
            </condition>
        </item>
        <item>
            <name>Beacon Light</name>
            <value>Off</value>
            <condition>
               <not><property>controls/lighting/beacon</property></not>
            </condition>
        </item>
        <item>
            <name>Electrical Power</name>
            <value>Establish</value>
        </item>
        <item>
            <name>Anti-Ice</name>
            <value>Off</value>
            <condition>
             <and>
              <equals>
               <property>controls/anti-ice/engin/antiice-knob</property>
               <value>0</value>
              </equals>
              <equals>
               <property>controls/anti-ice/engin[1]/antiice-knob</property>
               <value>0</value>
              </equals>
              <equals>
               <property>controls/anti-ice/wing-antiice-knob</property>
               <value>0</value>
              </equals>
             </and>
            </condition>
        </item>
        <item>
            <name>Fuel pump switches</name>
            <value>Off</value>
            <condition>
             <and>
              <not><property>controls/fuel/tank/boost-pump-switch</property></not>
              <not><property>controls/fuel/tank/boost-pump-switch[1]</property></not>
              <not><property>controls/fuel/tank[1]/boost-pump-switch</property></not>
              <not><property>controls/fuel/tank[1]/boost-pump-switch[1]</property></not>
              <not><property>controls/fuel/tank[2]/boost-pump-switch</property></not>
              <not><property>controls/fuel/tank[2]/boost-pump-switch[1]</property></not>
             </and>
            </condition>
        </item>
        <item>
          <name>Hydraulic Pumps</name>
          <value>Off</value>
          <condition>
           <and>
             <equals>
               <property>controls/hydraulics/system/LACMP-switch</property>
               <value type="int">0</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system/LENG_switch</property>
               <value type="bool">false</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[1]/C1ADP-switch</property>
               <value type="int">0</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[1]/C1ELEC-switch</property>
               <value type="bool">false</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[1]/C2ADP-switch</property>
               <value type="int">0</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[1]/C2ELEC-switch</property>
               <value type="bool">false</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[2]/RACMP-switch</property>
               <value type="int">0</value>
             </equals>
             <equals>
               <property>controls/hydraulics/system[2]/RENG_switch</property>
               <value type="bool">false</value>
             </equals>
           </and>
          </condition>
        </item>                          
        <item>
          <name>Window Heat</name>
          <value>Off</value>
          <condition>
           <and>
            <equals>
             <property>controls/anti-ice/window-heat-lf</property>
             <value>0</value>
            </equals>
            <equals>
             <property>controls/anti-ice/window-heat-ls</property>
             <value>0</value>
            </equals>
            <equals>
             <property>controls/anti-ice/window-heat-rf</property>
             <value>0</value>
            </equals>
            <equals>
             <property>controls/anti-ice/window-heat-rs</property>
             <value>0</value>
            </equals>
           </and>
          </condition>
        </item>
        <item>
          <name>ADIRU switch</name>
          <value>Off</value>
          <condition>
           <not><property>controls/flight/adiru</property></not>
          </condition>
        </item>
        <item>
          <name>EMER Lights Switch</name>
          <value>Off</value>
        </item>
        <item>
          <name>Audio Selector Panel</name>
          <value>Set</value>
        </item>
        <item>
          <name>Exterior Lights</name>
          <value>Off</value>
          <condition>
                <and>
                    <not><property>controls/lighting/taxi-lights</property></not>
                    <not><property>controls/lighting/landing-lights</property></not>
                    <not><property>controls/lighting/strobe</property></not>
                    <not><property>controls/lighting/nav-lights</property></not>
                    <not><property>controls/lighting/beacon</property></not>
                </and>
          </condition>          
        </item>
        <item>
            <name>APU</name>
            <value>Off</value>
            <condition>
                <equals>
                    <property>controls/APU/run</property>
                    <value>false</value>
                </equals>
            </condition>
        </item>    
    </checklist>
</PropertyList>
